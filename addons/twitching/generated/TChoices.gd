extends TBaseType
class_name TChoices

## Autogenerated. Do not modify.

## ID for the choice.
var id: String

## Text displayed for the choice.
var title: String

## Not used; will be set to 0.
var bits_votes: int

## Number of votes received via Channel Points.
var channel_points_votes: int

## Total number of votes received for the choice across all methods of voting.
var votes: int

## Create is similar to _init but takes parameters. Useful for using with autocomplete in the editor.
static func create(id: String, title: String, bits_votes: int, channel_points_votes: int, votes: int) -> TChoices:
	var _new = TChoices.new()
	_new.id = id
	_new.title = title
	_new.bits_votes = bits_votes
	_new.channel_points_votes = channel_points_votes
	_new.votes = votes
	return _new

## Create from object (usually returned from api)
static func from_object(obj: Variant) -> TChoices:
	if not obj: return
	if not obj is Dictionary:
		print("[TChoices]: Object is not Dictionary: ", obj)
		return

	var _new = TChoices.new()

	_new.id = obj.get("id") if obj.get("id") else ""
	_new.title = obj.get("title") if obj.get("title") else ""
	_new.bits_votes = obj.get("bits_votes") if obj.get("bits_votes") else 0
	_new.channel_points_votes = obj.get("channel_points_votes") if obj.get("channel_points_votes") else 0
	_new.votes = obj.get("votes") if obj.get("votes") else 0

	return _new
