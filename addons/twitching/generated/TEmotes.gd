extends TBaseType
class_name TEmotes

## Autogenerated. Do not modify.

## The index of where the Emote starts in the text.
var begin: int

## The index of where the Emote ends in the text.
var end: int

## The emote ID.
var id: String

## Create is similar to _init but takes parameters. Useful for using with autocomplete in the editor.
static func create(begin: int, end: int, id: String) -> TEmotes:
	var _new = TEmotes.new()
	_new.begin = begin
	_new.end = end
	_new.id = id
	return _new

## Create from object (usually returned from api)
static func from_object(obj: Variant) -> TEmotes:
	if not obj: return
	if not obj is Dictionary:
		print("[TEmotes]: Object is not Dictionary: ", obj)
		return

	var _new = TEmotes.new()

	_new.begin = obj.get("begin") if obj.get("begin") else 0
	_new.end = obj.get("end") if obj.get("end") else 0
	_new.id = obj.get("id") if obj.get("id") else ""

	return _new
